{
  "name": "2blocks",
  "version": "0.0.1",
  "description": "A game for those who keep their eyes on the street.",
  "main": "index.js",
  "scripts": {
    "test": "find . -name test -type d ! -path './node_modules/*' -exec mocha {} --compilers js:babel-core/register --recursive ';'",
    "build": "npm run lint && npm test && npm run pack",
    "pack": "webpack",
    "lint": "eslint src/ build/",
    "postlint": "echo 'Linting complete with no errors.'",
    "start": "sudo browser-sync start --server --index './build/index.html' --files './dist/*.js, ./build/*.html, ./build/*.js'",
    "watch": "watch 'npm run lint && npm run pack' ./src/ ./build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdbts/2Blocks.git"
  },
  "keywords": [
    "google",
    "maps",
    "nyc",
    "panoramas",
    "streetview"
  ],
  "author": "Vinny Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tdbts/2Blocks/issues"
  },
  "homepage": "https://github.com/tdbts/2Blocks#readme",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.12.8",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4",
    "watch": "^0.18.0",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "webpack": "^1.0.0"
  },
  "dependencies": {
    "redux": "^3.5.2"
  }
}
